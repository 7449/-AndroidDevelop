<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>


    <declare-styleable name="OldStatusLayout">
        <attr name="old_status_loading_layout" format="reference" />
        <attr name="old_status_empty_layout" format="reference" />
        <attr name="old_status_error_layout" format="reference" />
        <attr name="old_status_success_layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="StatusLayout">
        <attr name="status_error_layout" format="reference" />
        <attr name="status_success_layout" format="reference" />
        <attr name="status_empty_layout" format="reference" />
        <attr name="status_loading_layout" format="reference" />
        <attr name="status_normal_layout" format="reference" />

        <attr name="status_error_flag" format="boolean" />
        <attr name="status_success_flag" format="boolean" />
        <attr name="status_empty_flag" format="boolean" />
        <attr name="status_loading_flag" format="boolean" />
        <attr name="status_normal_flag" format="boolean" />
    </declare-styleable>
</resources>